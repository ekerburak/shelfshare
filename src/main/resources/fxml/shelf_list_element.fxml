<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<HBox alignment="CENTER" maxWidth="1400.0" minWidth="800.0" prefHeight="91.0" prefWidth="1200.0" style="-fx-background-color: #222222;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ShelfListElementController">
   <padding>
      <Insets bottom="20.0" left="40.0" right="40.0" top="20.0" />
   </padding>
   <children>
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <ImageView fitHeight="51.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/shelf_icon.png" />
               </image>
            </ImageView>
            <Label fx:id="shelfNameLabel" styleClass="text" stylesheets="@../assets/style.css" text="Fantastic Readers" />
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </HBox>
      <Region HBox.hgrow="ALWAYS" />
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <ImageView fitHeight="51.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/rate_star.png" />
               </image>
            </ImageView>
            <Label fx:id="RateLabel" styleClass="text" stylesheets="@../assets/style.css" text="Rate" />
         </children>
      </HBox>
   </children>
</HBox>
