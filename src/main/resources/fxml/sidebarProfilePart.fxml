<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" maxHeight="91.0" minHeight="80.0" prefHeight="91.0" prefWidth="400.0" style="-fx-background-color: #222222;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SidebarProfilePartController">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <ImageView fx:id="profileIcon" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/acount_circle.png" />
               </image>
            </ImageView>
            <Label fx:id="usernameLabel" styleClass="text" text="sewalT" textFill="#c2c2c2">
               <font>
                  <Font />
               </font>
            </Label>
         </children>
      </HBox>
      <Region HBox.hgrow="ALWAYS" />
      <ImageView fx:id="settingsIcon" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/settings.png" />
         </image>
      </ImageView>
   </children>
   <padding>
      <Insets bottom="20.0" left="30.0" right="30.0" top="20.0" />
   </padding>
</HBox>
